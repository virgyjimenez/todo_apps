<p id="notice"><%= notice %></p>

<p>
  <strong>Titulo:</strong>
  <%= @todo.title %>
</p>

<p>
  <strong>Notas:</strong>
  <%= @todo.notes %>
</p>

<%= link_to edit_todo_path(@todo), class: 'btn btn-info btn-lg' do %>
        <i class="glyphicon glyphicon-pencil"></i> Editar <% end %>
<%= link_to todos_path, class: 'btn btn-danger btn-lg' do %>
<i class="glyphicon glyphicon-repeat"></i> Regresar 
<% end %>